{
  "name": "Simple Optimization Example",
  "description": "A simple two-parameter, single-objective optimization example",
  "parameter_space": {
    "parameters": [
      {
        "name": "x1",
        "type": "continuous",
        "bounds": [-5.0, 5.0],
        "description": "First parameter"
      },
      {
        "name": "x2", 
        "type": "continuous",
        "bounds": [-5.0, 5.0],
        "description": "Second parameter"
      }
    ]
  },
  "objectives": [
    {
      "name": "minimize_function",
      "type": "minimize",
      "description": "Minimize the objective function"
    }
  ],
  "acquisition_function": "expected_improvement",
  "model_type": "gaussian_process",
  "n_initial_points": 10,
  "max_iterations": 50,
  "convergence_tolerance": 1e-6,
  "random_seed": 42,
  "parallel_evaluations": 1
}
